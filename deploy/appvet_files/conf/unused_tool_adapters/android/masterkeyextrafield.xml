<?xml version="1.0" encoding="UTF-8"?>

<appvet:ToolAdapter xmlns:appvet="https://appsec.nist.gov/appvet/"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="https://appsec.nist.gov/appvet/ToolAdapter.xsd ">

	<appvet:Description>
		<appvet:Name>MasterKey ExtraField Test</appvet:Name>
		<appvet:Id>masterkeyextrafield</appvet:Id>
		<appvet:Category>TESTTOOL</appvet:Category>
		<appvet:OS>ANDROID</appvet:OS>
		<appvet:VendorName>NIST</appvet:VendorName>
		<appvet:VendorWebsite>https://appsec.nist.gov/android-masterkey-extrafield-test
		</appvet:VendorWebsite>
		<appvet:ReportFile>HTML</appvet:ReportFile>
		<appvet:AppSubmit>APP_FILE</appvet:AppSubmit>
		<appvet:AuthenticationRequired>false</appvet:AuthenticationRequired>
	</appvet:Description>

	<appvet:Protocol>
		<appvet:Asynchronous>
			<appvet:Request>
				<appvet:URL>https://appsec.nist.gov/android-masterkey-extrafield-test/AsynchronousService
				</appvet:URL>
				<appvet:Method>POST</appvet:Method>
				<appvet:AuthenticationRequired>false</appvet:AuthenticationRequired>
				<appvet:Parameter>
					<appvet:Name>appid</appvet:Name>
					<appvet:Value>APPVET_DEFINED</appvet:Value>
				</appvet:Parameter>
				<appvet:Parameter>
					<appvet:Name>file</appvet:Name>
					<appvet:Value>APP_FILE</appvet:Value>
				</appvet:Parameter>
			</appvet:Request>
			<appvet:Receipt />
		</appvet:Asynchronous>
	</appvet:Protocol>
</appvet:ToolAdapter>
